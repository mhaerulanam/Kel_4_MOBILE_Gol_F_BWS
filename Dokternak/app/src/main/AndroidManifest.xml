<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.def.dokternak">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_dokternak"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_dokternak"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dokternak"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.konsultasi.kategoriHewan.CariKategoriActivity"></activity>
        <activity android:name=".ui.konsultasi.CariPetugasActivity" />
        <activity
            android:name=".ui.konsultasi.KonsultasiActivity"
            android:label="@string/title_activity_konsultasi"
            android:theme="@style/Theme.Dokternak.NoActionBar" />
        <activity
            android:name=".ui.artikel.DetailArtikelActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ui.profile.EditProfilActivity"/>
        <activity android:name=".ui.puskeswan.DetailPuskeswanActivity"/>
        <activity android:name=".ui.konsultasi.TulisKonsultasiActivity"/>
        <activity android:name=".ui.petugas.DetailPetugasActivity" />
        <activity android:name=".ui.konsultasi.DetailKonsultasiMasukActivity" />
        <activity android:name=".ui.konsultasi.DetailKonsultasiTekirimActivity" />
        <activity android:name=".ui.kontak.EditActivity" />
        <activity android:name=".ui.kontak.InsertActivity" />
        <activity android:name=".ui.register.RegisterActivity"/>
        <activity android:name=".ui.register.RegisterActivityBackup" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.splashscreen.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.welcome.WelcomeActivity" />
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>